#include <iostream>
#include <time.h>
using namespace std;
 
int main() {
    int num, number;
    srand(time(0));
    number=rand()%100+1;
    cout<<"Guess the number(1-100)"<<endl;
    cin>>num;
    if(num>number){
      cout<<"Number is too large"<<endl;
  }
    else if(num<number){
      cout<<"Number is too small"<<endl;
  }
    else{
      cout<<"You guessed the number correct "<<endl;
  }
  return 0;
}
